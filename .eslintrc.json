{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "radix": "off",
    "max-len": "off",
    "no-shadow": "off",
    "no-console": "off",
    "import/named": "off",
    "comma-dangle": "off",
    "linebreak-style": "off",
    "prefer-template": "off",
    "import/namespace": "off",
    "consistent-return": "off",
    "no-param-reassign": "off",
    "quotes": ["error", "double"],
    "no-restricted-syntax": "off",
    "import/no-mutable-exports": "off",
    "import/prefer-default-export": "off"
  }
}